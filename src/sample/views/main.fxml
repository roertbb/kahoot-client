   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="480.0" prefWidth="640.0">
         <children>
            <Button fx:id="createKahoot" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#createKahoot" text="Create Kahoot" />
            <Button fx:id="joinKahoot" layoutX="148.0" layoutY="14.0" mnemonicParsing="false" onAction="#joinKahoot" text="Join Kahoot" />
         </children>
      </Pane>
   </children>
